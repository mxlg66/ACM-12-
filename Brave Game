#include<stdio.h>
#include<string.h>
#include<stdlib.h>
#include<algorithm>
#include<string.h>
using namespace std; 
int main()
{
    int C, N, M;
    scanf("%d", &C);
    while (C--)
    {
        scanf("%d%d", &N, &M);
        if (N % (M + 1) == 0) printf("second\n");
        else printf("first\n");
    }
    return 0;
}
